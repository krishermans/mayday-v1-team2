{
  "plugins": [
    "office-addins"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "extends": [
    "plugin:office-addins/react"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "object-curly-spacing": ["error", "always"],
    "prettier/prettier": 0,
    "sort-keys": ["error", "asc", {"caseSensitive": true, "natural": false, "minKeys": 2}]
  }
}